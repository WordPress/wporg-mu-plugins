{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "wporg/multisite-latest-posts",
	"version": "0.1.0",
	"title": "Multisite Latest Posts",
	"category": "widgets",
	"icon": "editor-ul",
	"description": "A block for use across the whole wp.org network.",
	"textdomain": "wporg",
	"editorScript": "file:./index.js",
	"style": "file:./style.css",
	"attributes": {
		"blogId" : {
			"type": "integer",
			"default": -1
		},
		"perPage": {
			"type": "integer",
			"default": 3
		}
	}
}